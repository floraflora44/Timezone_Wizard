{
  "name": "timezone-wizard",
  "version": "1.0.0",
  "description": "Find the perfect meeting time across timezones",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "start": "vite preview --port $PORT",
    "vercel-build": "vite build"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "dependencies": {
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0"
  },
  "devDependencies": {
    "vite": "^4.4.0",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "vitest": "^0.34.0"
  },
  "keywords": ["timezone", "meeting", "scheduler", "global", "teams"],
  "author": "Timezone Wizard",
  "license": "MIT"
}
